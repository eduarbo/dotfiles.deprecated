{
  "title": "Personal rules (@eduarbo)",
  "rules": [
    {
      "description": "Thumb cluster",
      "manipulators": [
        {
          "description": "Change spacebar to right_command. (Post return if pressed alone)",
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_command"
            }
          ],

          "to_if_alone": [
            {
              "key_code": "return_or_enter"
            }
          ]
        },
        {
          "description": "Change right_command to right_shift. (Post spacebar if pressed alone)",
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_shift"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ]
        },
        {
          "description": "Change left_command to left_shift. (Post tab if pressed alone)",
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_shift"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "spacebar"
            }
          ]
        },
        {
          "description": "Change right_option to right_option. (Post escape if pressed alone)",
          "type": "basic",
          "from": {
            "key_code": "right_option",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_option"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "0",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        },
        {
          "description": "Change left_option to right_control",
          "type": "basic",
          "from": {
            "key_code": "left_option",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_control"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "9",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        },
        {
          "description": "Change left_control to fn.",
          "type": "basic",
          "from": {
            "key_code": "left_control",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "fn"
            }
          ]
        }
      ]
    },
